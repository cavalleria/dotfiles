{
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings.global": {
      "i": false,
      "<c-o>": "noop",
      "*": "toggleSelection",
      "<tab>": "actionMenu",
      "o": ["wait", "expanded?", "collapse", "expand"],
      "<cr>": ["wait", "expandable?", "cd", "open"],
      "<bs>": ["wait", "gotoParent"],
      "e": "open",
      "s": "open:split",
      "E": "open:vsplit",
      "t": "open:tab",
      "ic": "preview:content",
      "il": "preview:labeling",
      "Il": "previewOnHover:toggle:labeling",
      "Ic": "previewOnHover:toggle:content",
      "II": "previewOnHover:disable",
      "yp": "copyFilepath",
      "yn": "copyFilename",
      "yy": "copyFile",
      "p": "pasteFile",
      "a": "addFile",
      "A": "addDirectory",
      "r": "rename",
      "R": "refresh",
      "f": "search",
      "F": "searchRecursive",
      "q": "quit"
  },
  "explorer.width": 40,
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.git.enable": false,
  "explorer.buffer.tabOnly": true,
  "explorer.file.reveal.whenOpen": false,
  "explorer.file.reveal.filter": {
      "literals": ["node_modules"]
  },
  "pyright.disableDiagnostics": true,
  "clangd.disableDiagnostics": true
}
